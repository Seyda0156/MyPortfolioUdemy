@model Message
@{
    ViewData["Title"] = "MessageDetail";
    Layout = "~/Views/Layout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Deneyimler</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Mesaj  Detaylar</div>
					</div>
					<div class="card-body">
						<div class="card-sub">
							Gönderen: <b>@Model.NameSurname</b>
							<br />
							Email: <b>@Model.Email</b>
							<br />
							Tarih: <b>@Model.SendDate</b>
							<br />
							Mesaj: <b>@Model.MessageDetail</b>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
